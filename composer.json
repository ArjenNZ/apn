{
    "name": "fruitcake/laravel-apn-notification-channel",
    "description": "Apple APN Push Notification Channel",
    "homepage": "https://github.com/fruitcake/laravel-apn-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Fruitcake",
            "email": "info@fruitcake.nl",
            "homepage": "https://fruitcake.nl"
        }
    ],
    "require": {
        "php": ">=5.5",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3",
        "zendframework/zendservice-apple-apns": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Fruitcake\\NotificationChannels\\Apn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fruitcake\\NotificationChannels\\Apn\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
